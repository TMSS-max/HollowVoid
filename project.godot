[application]
config/name="Hollow Void"
config/description="A Hollow Knight-style adventure game"
config/version="1.0.0"
config/argv/return_warnings=true

[display]
window/width=720
window/height=1280
window/fullscreen=false
window/resizable=true
window/vsync=true

[input]
actions/move_left/0= "ui_left"
actions/move_right/0= "ui_right"
actions/jump/0= "ui_accept"
actions/crouch/0= "ui_cancel"
actions/attack/0= "ui_select"
actions/attack/0/1= "touch_attack"

actions/touch_attack/0= "touch_attack"

[physics]
default_fps=60
use_default_step=true

